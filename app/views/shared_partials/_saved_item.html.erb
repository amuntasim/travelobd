<% if @already_saved %>
    <%= link_to ' [ ' + t('general.label.remove') + ' ] ', 'javascript:void(0)', :class => 'remove_from_save_listing', :sl_id=> @already_saved.id, :savable_id => savable.id, :savable_type => savable.class.name %>  <%= link_to t('general.label.already_saved'), my_saved_listings_path %>  &nbsp;
<% else %>
    <%= link_to t('helpers.submit.save'), 'javascript:void(0)', :class => 'save_item', :savable_type => savable.class.name, :item_id=> savable.id %>
<% end %>