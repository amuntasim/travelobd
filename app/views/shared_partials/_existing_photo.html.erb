<div class="edit_box_img existing_image">
  <div class="e_bor">
    <%= image_tag existing_photo.photo.url(:thumb), :id => "asset_#{existing_photo.id}", :title => existing_photo.label %>
  </div>
  <%= link_to t('helpers.delete'), 'javascript:void(0)', :asset_id => existing_photo.id, :class => 'del_photo' %>
  <%= link_to t('general.label.edit_label'), 'javascript:void(0)', :id => "asset_link_#{existing_photo.id}", :asset_id => existing_photo.id, :class => 'edit_asset_label', :title => existing_photo.label %>
  <%= check_box_tag "main_check_#{existing_photo.id}", existing_photo.id, existing_photo.main?, :asset_id =>existing_photo.id, :class=> 'mainCh' %>
  <%= t('general.label.main_image') %>
</div>