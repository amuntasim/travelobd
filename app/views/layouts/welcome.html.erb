<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <title><%= @title || "Welcome" %></title>
  <%= stylesheet_link_tag 'style', 'menu', 'bx_styles' %>
  <%= javascript_include_tag 'jquery.min', 'jquery-ui', 'jquery.validate', 'rails', 'menu', 'application', 'jquery.bxSlider.min' %>
  <%= yield :js_css_include %>
  <%= include_tiny_mce_if_needed %>
  <%= csrf_meta_tag %>
</head>

<body>
<div class="wrapper">
  <div class="wrap_body">
    <!--heading start -->
    <%= render 'layouts/header' %>
    <!--heading end -->
    <!--middle start -->

    <div class="midContent">
      <div class="home_left fl">
        <%= render :partial => 'shared/left_panel' %>
      </div>

      <%= yield %>
    </div>

  </div>
  <!--<div class="mid_bot_bg"></div>-->
  <!--middle end -->

  <!--footer start -->
  <%= render :partial => 'layouts/footer' %>

  <!--footer start -->


</div>
</body>
</html>
