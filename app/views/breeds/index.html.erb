<h1 class="admin_header">Listing breeds</h1>
<div class="admin_new_link"><%= link_to 'New Breed', new_breed_path %></div>

<div class="msg_desc_info_big">

  <div class="f_bor evenRow ">
    <div class="fl w_200 pad_lr_5">
      <strong>Name</strong>
    </div>
    <div class="fl w_130 pad_lr_5">
      
    </div>
    <div class="fl w_130 pad_lr_5">
      <strong></strong>
    </div>
    <div class="admin_links">

    </div>
  </div>

  <% @breeds.each do |breed| %>

    <div class="f_bor <%= cycle('oddROw', 'evenRow') %> ">
      <div class="fl w_200 pad_lr_5">
        <%= breed.name %>
      </div>

      <div class="fl w_130 pad_lr_5">
        &nbsp;
      </div>
      <div class="fl w_130 pad_lr_5">
        &nbsp;
      </div>


      <div class="fl admin_links">
        <span><%= link_to 'Show', breed %></span>
        <span><%= link_to 'Edit', edit_breed_path(breed) %></span>
        <span><%= link_to 'Destroy', breed, :confirm => 'Are you sure?', :method => :delete %></span>
      </div>
    </div>

  <% end %>

</div>

