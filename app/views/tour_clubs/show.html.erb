<% title @tour_club.name %>
<%= content_for :js_css_include do %>
    <%= javascript_include_tag 'jquery.easing.1.3', 'jquery.galleryview-1.1', 'jquery.timers-1.1.2', 'thickbox' %>
    <%= javascript_include_tag 'jquery.validate' %>
    <%= stylesheet_link_tag 'thickbox' %>
<% end %>

<div class="pageHead">
  <%= @tour_club.name %>
</div>

<div class="item_details">
  <div class="itemDetail">
    <%= @tour_club.description %>
  </div>
  <%= render :partial => 'shared_partials/detail_page_comment', :locals => {:item => @tour_club, :rating => true} %>
</div>
